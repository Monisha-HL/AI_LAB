from copy import deepcopy

def is_goal(state, goal):
    return state == goal
def find_blank(state):
    for i in range(3):
        for j in range(3):
            if state[i][j] == 0:
                return i, j
def get_neighbors(state):
    neighbors = []
    x, y = find_blank(state)
    directions = [(0, -1), (0, 1), (-1, 0), (1, 0)]
    for dx, dy in directions:
        nx, ny = x + dx, y + dy
        if 0 <= nx < 3 and 0 <= ny < 3:
            new_state = deepcopy(state)
            new_state[x][y], new_state[nx][ny] = new_state[nx][ny], new_state[x][y]
            neighbors.append(new_state)
    return neighbors
def dfs_limited(initial, goal, depth_limit):
    stack = [(initial, [], 0)]
    visited = set()

    while stack:
        current, path, depth = stack.pop()
        state_tuple = tuple(map(tuple, current))
        if state_tuple in visited:
            continue
        visited.add(state_tuple)

        if is_goal(current, goal):
            return path + [current]

        if depth < depth_limit:
            for neighbor in get_neighbors(current):
                stack.append((neighbor, path + [current], depth + 1))
    return None
def iterative_deepening_search(initial, goal, max_depth=5):
    for depth in range(max_depth):
        solution = dfs_limited(initial, goal, depth)
        if solution:
            return solution
    return None
def print_state(state):
    for row in state:
        print(row)
    print()
def get_input_state(prompt):
    print(prompt)
    state = []
    for i in range(3):
        row = list(map(int, input(f"Enter row {i+1} (3 numbers separated by space): ").split()))
        state.append(row)
    return state
if __name__ == "__main__":
    initial = get_input_state("Enter the initial state:")
    goal = get_input_state("Enter the goal state:")
    solution = iterative_deepening_search(initial, goal)

    if solution:
        print("\nSolution found in", len(solution) - 1, "moves:")
        for step in solution:
            print_state(step)
    else:
        print("\nNo solution found.")
